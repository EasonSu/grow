{
  "name": "ventures/woorelease-extension-commands",
  "description": "Ventures extension commands for Woorelease",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/woocommerce/woorelease"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "woocommerce/woorelease": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "WooCommerce\\Ventures\\WR\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": "test -L \"../extensions/bootstrap.php\" || ( mkdir -p ../extensions && ln -s \"$(pwd)/bootstrap.php\" ../extensions/bootstrap.php )"
  }
}
